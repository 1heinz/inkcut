from enaml.workbench.plugin_manifest import PluginManifest
from enaml.workbench.api import Extension
from inkcut.workbench.core.device import DeviceDriver

def generic_factory(protocol):
    from inkcut.plugins.devices.generic import GenericDevice
    return GenericDevice(protocol=protocol)

